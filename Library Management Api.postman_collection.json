                "exec": [
                  "const response = pm.response.json();\r",
                  "\r",
                  "if (response.success && response.data && response.data.token) {\r",
                  "    pm.environment.set(\"token\", response.data.token);\r",
                  "    console.log(\"Token guardado exitosamente:\", response.data.token);\r",
                  "} else {\r",
                  "    console.log(\"No se pudo guardar el token - respuesta inválida\");\r",
